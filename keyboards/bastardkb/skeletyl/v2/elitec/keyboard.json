{
    "keyboard_name": "Skeletyl Elite-C",
    "usb": {
        "device_version": "2.0.0"
    },
    "features": {
        "bootmagic": true,
        "mousekey": true,
        "extrakey": true,
        "rgb_matrix": true
    },
    "ws2812": {
        "pin": "D3"
    },
    "rgb_matrix": {
        "driver": "ws2812"
    },
    "matrix_pins": {
        "cols": ["B4", "E6", "C6", "B1", "B3"],
        "rows": ["B5", "F7", "F6", "B6"]
    },
    "diode_direction": "COL2ROW",
    "split": {
        "enabled": true,
        "soft_serial_pin": "D0"
    },
    "processor": "atmega32u4",
    "bootloader": "atmel-dfu"
}
